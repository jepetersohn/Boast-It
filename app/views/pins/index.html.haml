#pins.transitions-enabled
  - @pins.each do |pin|
    .box.panel.panel-default
      = link_to (image_tag pin.image.url), pin
      .panel-body
        %h2= link_to pin.title, pin
        %h4= link_to pin.category.name, user_category_url(pin.user_id, pin.category_id)
        %p.user
        Submitted by
        = pin.user.email